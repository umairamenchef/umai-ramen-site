{
  "name": "gulp-starter-kit",
  "version": "3.0.0",
  "private": true,
  "description": "Gulp Starter Kit for front-end developers.",
  "bugs": {
    "url": "https://github.com/nstungcom/gulp-starter-kit/issues",
    "email": "hi@nstung.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nstungcom/gulp-starter-kit.git"
  },
  "license": "MIT",
  "author": "Son Tung Ngo <hi@nstung.com>",
  "main": "gulpfile.babel.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm-run-all clean --parallel eleventy:watch gulp:dev || exit 0",
    "build": "npm-run-all clean --parallel eleventy:build gulp:build",
    "eleventy:watch": "eleventy --quiet --watch || exit 0",
    "eleventy:build": "eleventy || exit 0",
    "gulp:dev": "gulp --silent || exit 0",
    "gulp:build": "cross-env NODE_ENV=production gulp build || exit 0",
    "fix-css": "stylelint \"src/**/*.{css,scss}\" --fix || exit 0",
    "fix-js": "eslint \"src/**/*.js\" --fix || exit 0"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "autoprefixer": "^9.7.4",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-gzip": "^1.4.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^2.1.0",
    "gulp-rev-all": "^2.0.2",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "js-beautify": "^1.10.3",
    "js-yaml": "^3.13.1",
    "markdown-it": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.14.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
